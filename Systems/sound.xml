<?xml version="1.0"?>
<PropertyList>
	<filter>
		<debug>false</debug>
		<type>gain</type>
		<input>
			<expression>
				<table>
					<property>sim/model/door-positions/frontwin/position-norm</property>
					<entry><ind>0.0</ind><dep>0.6</dep></entry>
					<entry><ind>0.1</ind><dep> 0.85</dep></entry>
					<entry><ind>1.0</ind><dep> 1</dep></entry>
				</table>
			</expression>
		</input>
		<output>/sim/model/front-win-volume</output>
		<gain>1</gain>
	</filter>
	<filter>
		<debug>false</debug>
		<type>gain</type>
		<input>
			<expression>
				<table>
					<property>sim/model/door-positions/rearwin/position-norm</property>
					<entry><ind>0.0</ind><dep>0.6</dep></entry>
					<entry><ind>0.1</ind><dep> 0.85</dep></entry>
					<entry><ind>1.0</ind><dep> 1</dep></entry>
				</table>
			</expression>
		</input>
		<output>/sim/model/rear-win-volume</output>
		<gain>1</gain>
	</filter>
	
</PropertyList>
